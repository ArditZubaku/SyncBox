<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.WebView?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="597.0" prefWidth="870.0"
    xmlns="http://javafx.com/javafx/17.0.2-ea">
    <children>
        <MenuBar prefHeight="28.0" prefWidth="871.0" AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0"
            AnchorPane.topAnchor="0.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Open" />
                        <MenuItem mnemonicParsing="false" text="Close" />
                        <MenuItem mnemonicParsing="false" onAction="#addAccountAction"
                            text="Add account" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#optionsAction" text="Options" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TreeView fx:id="emailsTreeView" layoutY="28.0" prefHeight="573.0" prefWidth="200.0"
            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="28.0" />
        <TableView fx:id="emailTableView" layoutX="200.0" layoutY="28.0" prefHeight="200.0"
            prefWidth="676.0"
            AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="28.0">
            <columns>
                <TableColumn prefWidth="136.0" text="Sender" />
                <TableColumn prefWidth="152.0" text="Subject" />
                <TableColumn prefWidth="144.0" text="Recipient" />
                <TableColumn prefWidth="102.0" text="Size" />
                <TableColumn prefWidth="138.0" text="Date" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <WebView fx:id="emailWebView" layoutX="200.0" layoutY="228.0" prefHeight="372.0"
            prefWidth="671.0"
            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0"
            AnchorPane.topAnchor="228.0" />
    </children>
</AnchorPane>