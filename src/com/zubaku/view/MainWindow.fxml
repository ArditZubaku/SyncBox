<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>


<AnchorPane prefHeight="597.0" prefWidth="870.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zubaku.controller.MainWindowController">
   <children>
      <MenuBar layoutY="1.0" prefHeight="27.0" prefWidth="870.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" text="Open" />
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#optionsAction" text="Options" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TreeView fx:id="emailsTreeView" layoutY="28.0" prefHeight="571.0" prefWidth="200.0" />
      <TableView fx:id="emailTableView" layoutX="200.0" layoutY="28.0" prefHeight="200.0" prefWidth="671.0">
        <columns>
          <TableColumn prefWidth="136.0" text="Sender" />
          <TableColumn prefWidth="152.0" text="Subject" />
            <TableColumn prefWidth="144.0" text="Recipient" />
            <TableColumn prefWidth="102.0" text="Size" />
            <TableColumn prefWidth="135.0" text="Date" />
        </columns>
      </TableView>
      <WebView fx:id="emailWebView" layoutX="200.0" layoutY="228.0" prefHeight="372.0" prefWidth="671.0" />
   </children>
</AnchorPane>
